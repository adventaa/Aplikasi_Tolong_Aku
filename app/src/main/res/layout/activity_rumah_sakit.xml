<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#770202"
    tools:context=".Rumah_sakit">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="67dp"
        android:layout_height="62dp"
        android:background="#770202"
        app:srcCompat="@drawable/skip_back_2"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="1dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="191dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="116dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="486dp"
        app:srcCompat="@drawable/rumah_sakit"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="111dp" />

    <Button
        android:id="@+id/button_dokter"
        android:layout_width="188dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="114dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="108dp"
        android:layout_marginBottom="319dp"
        android:background="@drawable/shape_button"
        android:backgroundTint="@color/white"
        android:text="Dokter"
        android:textColor="@color/white"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/black"
        app:iconTint="@color/white" />

    <Button
        android:id="@+id/button_ambulance"
        android:layout_width="189dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="114dp"
        android:layout_marginTop="453dp"
        android:layout_marginEnd="108dp"
        android:layout_marginBottom="212dp"
        android:text="Ambulance"
        android:textColor="@color/white" />

</RelativeLayout>