<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pemadam">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="329dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="402dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/boxputih3" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/input_layout_nama"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="339dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="468dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="196dp"
        android:hint="Masukkan nama"/>

    <Button
        android:id="@+id/button_tolong"
        android:layout_width="179dp"
        android:layout_height="275dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="122dp"
        android:layout_marginTop="596dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/shape_button"
        android:backgroundTint="@color/white"
        android:text="Tolong Aku!"
        android:textColor="@color/white"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/black"
        app:iconTint="@color/black" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="73dp"
        android:layout_height="78dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="413dp"
        android:layout_marginEnd="318dp"
        android:layout_marginBottom="400dp"
        app:srcCompat="@drawable/pemadam"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="111dp" />

    <TextView
        android:id="@+id/TimSar"
        android:layout_width="265dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="103dp"
        android:layout_marginTop="404dp"
        android:layout_marginEnd="38dp"
        android:text="Pemadam Kebakaran"
        android:textColor="@color/black"
        android:textSize="38sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/nama"
        android:layout_width="346dp"
        android:layout_height="64dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="375dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="161dp"
        android:background="@drawable/backgrounlogin"
        android:hint="Masukkan Nama "
        android:paddingLeft="43dp" />

    <EditText
        android:id="@+id/nomor"
        android:layout_width="346dp"
        android:layout_height="64dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="375dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="89dp"
        android:background="@drawable/backgrounlogin"
        android:hint="Masukkan Nomor Telepon "
        android:paddingLeft="43dp" />

    <org.osmdroid.views.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="403dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="327dp"
        android:layout_marginEnd="300dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="420dp"
        android:background="@drawable/backbutton"
        app:srcCompat="@drawable/skip_back_2"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="1dp" />

</RelativeLayout>